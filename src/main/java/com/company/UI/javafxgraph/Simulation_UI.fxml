<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="com.company.UI.EPKUI.UI_SIMULATION_MANAGEMENT">
    <top>
        <VBox maxHeight="65.0" maxWidth="800.0" minHeight="65.0" minWidth="800.0" prefHeight="65.0" prefWidth="800.0"
              BorderPane.alignment="CENTER">
            <children>
                <TextArea editable="false" maxHeight="55.0" maxWidth="790.0" minHeight="55.0" minWidth="790.0"
                          prefHeight="55.0" prefWidth="790.0"
                          text="This window provides a management tool for resources participating in the EPC simulation.&#10;After that, new resources can be bound to functions to be used in started processes."
                          wrapText="true">
                    <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </VBox.margin>
                    <font>
                        <Font name="Arial" size="12.0"/>
                    </font>
                </TextArea>
            </children>
        </VBox>
    </top>
    <left>
        <VBox maxHeight="680.0" maxWidth="490.0" minHeight="680.0" minWidth="490.0" prefHeight="680.0" prefWidth="490.0"
              BorderPane.alignment="CENTER">
            <children>
                <VBox fx:id="Choosebox" maxHeight="300.0" maxWidth="490.0" minHeight="300.0" minWidth="490.0"
                      prefHeight="300.0" prefWidth="490.0">
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                </VBox>
                <Separator maxWidth="290.0" minWidth="290.0" prefWidth="290.0"/>
                <VBox fx:id="Showbox" maxHeight="380.0" maxWidth="490.0" minHeight="380.0" minWidth="490.0"
                      prefHeight="380.0" prefWidth="490.0"/>
            </children>
            <BorderPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </BorderPane.margin>
        </VBox>
    </left>
    <right>
        <VBox fx:id="Editbox" maxWidth="690.0" minWidth="690.0" prefHeight="200.0" prefWidth="690.0"
              BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets top="5.0"/>
            </BorderPane.margin>
        </VBox>
    </right>
    <center>
        <Separator orientation="VERTICAL" prefHeight="200.0" BorderPane.alignment="CENTER"/>
    </center>
    <bottom>
        <VBox maxHeight="35.0" maxWidth="1190.0" minHeight="35.0" minWidth="1190.0" prefHeight="35.0" prefWidth="1190.0"
              BorderPane.alignment="CENTER">
            <children>
                <Separator minWidth="1200.0" prefWidth="200.0"/>
                <ButtonBar maxHeight="35.0" maxWidth="1190.0" minHeight="35.0" minWidth="1190.0" prefHeight="35.0"
                           prefWidth="1190.0">
                    <buttons>
                        <Button fx:id="OK_Button" cancelButton="true" defaultButton="true" mnemonicParsing="false"
                                onAction="#actionPerformed" text="Save"/>
                        <Button fx:id="CANCEL_Button" cancelButton="true" defaultButton="true" layoutX="725.0"
                                layoutY="15.0" mnemonicParsing="false" onAction="#actionPerformed" text="Cancel"/>
                    </buttons>
                </ButtonBar>
            </children>
            <BorderPane.margin>
                <Insets right="5.0" top="5.0"/>
            </BorderPane.margin>
        </VBox>
    </bottom>
</BorderPane>
